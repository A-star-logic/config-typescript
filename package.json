{
  "name": "@ansearch/config",
  "version": "0.0.0",
  "packageManager": "yarn@4.5.0",
  "description": "The various configs used to code in Typescript at A* Logic",
  "homepage": "https://github.com/A-star-logic/config-typescript",
  "bugs": {
    "url": "https://github.com/A-star-logic/config-typescript/issues"
  },
  "license": "MIT",
  "author": "A* Logic (https://astarlogic.com)",
  "devDependencies": {
    "@types/node": "^22.7.4",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "exports": {
    "./backend/*": {
      "types": "./backend/*",
      "default": "./backend/*"
    },
    "./frontend/*": {
      "types": "./frontend/*",
      "default": "./frontend/*"
    },
    "./library/*": {
      "types": "./library/*",
      "default": "./library/*"
    }
  },
  "bin": {
    "npmjs-version-verify": "./bin/version-check.js"
  },
  "scripts": {
    "build": "tsc",
    "verify": "node bin/version-check.mjs \"$(npm view @mab-tech/config version)\" ../package.json"
  },
  "type": "module"
}
